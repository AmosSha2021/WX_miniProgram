<view class="map">
  <!-- 地图容器 -->
  <map 
    id="map" 
    longitude="{{longitude}}" 
    latitude="{{latitude}}" 
    scale="16"
    markers="{{markers}}"
    style="width: 100%; height: 1000rpx;">
  </map>
</view>

<!-- 打卡按钮 -->
<view class="btn-container">

  <button type="primary" bindtap="handleCheckIn">立即打卡</button>
</view>